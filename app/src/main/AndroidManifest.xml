<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobsoft.mobsoft">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:name=".MobSoftApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="5de60eb4f9eeee7505ffb2db0f9c2c13a3a5f7c0" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.event.EventActivity" />
        <activity android:name=".ui.eventCreate.EventCreateActivity" />
        <activity android:name=".ui.userEdit.UserEditActivity" />
        <activity android:name=".ui.userList.UserListActivity"></activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
</manifest>